{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/Input.jsx",
    "screens/RegistrationScreen.jsx",
    "screens/LoginScreen.jsx",
    "app/index.tsx"
  ]
}
